{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Snoozeless",
      "type": "pwa-node",
      "request": "launch",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/ts-node",
      "runtimeArgs": [
        "-r",
        "tsconfig-paths/register"
      ],
      "args": [
        "${workspaceFolder}/src/index.ts"
      ],
      "cwd": "${workspaceFolder}",
      "skipFiles": [
        "node_modules/**"
      ]
    }
  ]
}
